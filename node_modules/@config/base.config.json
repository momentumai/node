{
    "workerToken": "workerToken",
    "AWS": {
        "region": "us-east-1"
    },
    "notification": {
        "upMargin": 45,
        "downMargin": 40,
        "slack": "https://hooks.slack.com/services/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"
    },
    "logLevel": "info",
    "db": {
        "socketPath": null,
        "charset": "UTF8_GENERAL_CI",
        "timezone": "local",
        "connectTimeout": 10000,
        "stringifyObjects": false,
        "insecureAuth": false,
        "typeCast": true,
        "supportBigNumbers": true,
        "bigNumberStrings": true,
        "dateStrings": false,
        "debug": false,
        "trace": true,
        "multipleStatements": false,
        "acquireTimeout": 10000,
        "waitForConnections": true,
        "connectionLimit": 1,
        "queueLimit": 0,
        "host": null,
        "port": null,
        "user": null,
        "password": null,
        "database": null
    },
    "memcache": {
        "addr": "localhost",
        "port": "11211"
    },
    "frontend": {
        "facebook": {
            "scope": [
                "manage_pages",
                "ads_management",
                "publish_pages"
            ]
        },
        "ga": {
            "id": "UA-64108533-2",
            "options": {
                "cookieDomain": "none"
            },
            "debug": 1
        }
    },
    "tracking": {
        "prefix": "#:"
    },
    "sessionTimeout": 31536000,
    "trackingCode": "<script>\n(function(b,r,a,n,d){d = document.createElement('script');d.async=1;d.src=b;document.getElementsByTagName('head')[0].appendChild(d);d.onload=function(){MomentumBasic.Main.init(r);};})('//storage.googleapis.com/momentum-tracking/basic.js',{id:'{{team_id}}'});\n</script>",
    "rules": [
        {
            "module": "top_not_alerted",
            "name": "default alerts",
            "definition": {
                "OR": [{
                    "momentum": {
                        "GT": 45
                    }
                }],
                "action": ["alert_make", "slack"]
            }
        }, {
            "module": "alerted",
            "name": "default clear alerts",
            "definition": {
                "OR": [{
                    "momentum": {
                        "LT": 40
                    }
                }],
                "action": ["alert_remove", "promotion_stop"]
            }
        }
    ]
}
